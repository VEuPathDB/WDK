{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "properties": {
    "analysisName": {
      "type": "string"
    },
    "answerValueHash": {
      "type": "string"
    },
    "formParams": {
      "type": "object",
      "patternProperties": {
        "^.+$": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "analysisId": {
      "type": "integer"
    },
    "stepId": {
      "type": "integer"
    },
    "displayName": {
      "type": "string"
    },
    "shortDescription": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "hasParams": {
      "type": "boolean"
    },
    "status": {
      "type": "string"
    },
    "invalidStepReason": {
      "type": ["string", "null"]
    }
  },
  "required": [
    "analysisName",
    "answerValueHash",
    "formParams",
    "analysisId",
    "stepId",
    "displayName",
    "shortDescription",
    "description",
    "hasParams",
    "status",
    "invalidStepReason"
  ]
}
