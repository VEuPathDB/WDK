#!/usr/bin/perl

use lib "$ENV{GUS_HOME}/lib/perl";
use WDK::Model::CommandHelper;

my $GUS_HOME = $ENV{GUS_HOME};
my $CLASSPATH = &WDK::Model::CommandHelper::getJavaClasspath($GUS_HOME);
my $sysProps = &WDK::Model::CommandHelper::getSystemProps($GUS_HOME, 'wdkSanityTest');
my $args = &WDK::Model::CommandHelper::getJavaArgs(@ARGV);
my $envJvmOpts = $ENV{GUSJVMOPTS};

my $cmd = "java $sysProps $envJvmOpts -classpath $CLASSPATH org.gusdb.wdk.model.test.SanityTester $args";

system($cmd);

exit $? >> 8;
